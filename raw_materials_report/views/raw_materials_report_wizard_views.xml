<!--                        <field name="line[0][comp]" string="Постачальник"/>-->
<!--                        <field name="partner" string="Водій"/>-->
<!--                        <field name="quantity" string="Об'єм (м3)"/>-->
<!--                        <field name="pichA" string="Сорт A"/>-->
<!--                        <field name="pichB" string="Сорт B"/>-->
<!--                        <field name="pichC" string="Сорт C"/>-->
<!--                        <field name="pichD" string="Сорт D"/>-->
<!--                    </tree>-->
<!--                </field>-->

                <footer>
                    <button name="action_gen_report"
                            string="Друкувати"
                            type="object"
                            class="btn-primary"/>
                    <button special="cancel"
                            string="Скасувати"
                            class="btn-secondary"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
































