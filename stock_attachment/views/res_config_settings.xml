<odoo>
    <record id="res_config_settings_view_form_stock_m" model="ir.ui.view">
		<field name="name">res.config.settings.view.form.inherit.purchase.stock.m</field>
		<field name="model">res.config.settings</field>
		<field name="inherit_id" ref="stock.res_config_settings_view_form"/>
		<field name="arch" type="xml">
            <xpath expr="//block[@id='schedule_info']" position="inside">
                <setting>
                    <field name="recycling_rates"/>
                </setting>
                <setting>
                    <field name="minimum_stock"/>
                </setting>
            </xpath>
        </field>
    </record>
</odoo>